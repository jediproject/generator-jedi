    "scripts": {
        "develop": "npm install && node node_modules/bower/bin/bower install && node node_modules/gulp/bin/gulp.js",
        "release": "npm install && node node_modules/bower/bin/bower install && node node_modules/gulp/bin/gulp.js build --env=release",
        "master": "npm install && node node_modules/bower/bin/bower install && node node_modules/gulp/bin/gulp.js build --env=master",
        "start": "npm install && node node_modules/bower/bin/bower install && node node_modules/gulp/bin/gulp.js && <% if (props.generateAuth) {%>start node node_modules/apimocker/bin/apimocker.js -c mocks/config.json && <%}%>node node_modules/gulp/bin/gulp.js serve",
        "start-nix": "npm install && node node_modules/bower/bin/bower install && node node_modules/gulp/bin/gulp.js && (<% if (props.generateAuth) {%>node node_modules/apimocker/bin/apimocker.js -c mocks/config.json & <%}%>node node_modules/gulp/bin/gulp.js serve)"
    },
    "jshintConfig": {
        "globalstrict": true,
        "curly": true,
        "eqeqeq": true,
        "eqnull": true,
        "browser": true,
        "strict": true,
        "newcap": false,
        "globals": {
            "jQuery": true,
            "angular": false,
            "$": false,
            "_": false,
            "jd": false,
            "define": false,
            "console": false,
            "moment": false
        }
    }
