    "scripts": {
        "develop": "npm install && node node_modules/bower/bin/bower install && gulp",
        "release": "npm install && node node_modules/bower/bin/bower install && gulp build --env=release",
        "master": "npm install && node node_modules/bower/bin/bower install && gulp build --env=master",
